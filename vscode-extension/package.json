{
  "name": "rust-markdown-notebook",
  "displayName": "Rust Markdown Notebook",
  "description": "Rust notebooks in plain Markdown",
  "version": "0.0.1",
  "publisher": "voxlet",
  "repository": "https://github.com/voxlet/rust-markdown-notebook",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:rust-markdown-notebook"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rust-markdown-notebook.helloWorld",
        "title": "Hello World"
      }
    ],
    "notebooks": [
      {
        "id": "rust-markdown-notebook",
        "type": "rust-markdown-notebook",
        "displayName": "Rust Markdown Notebook",
        "selector": [
          {
            "filenamePattern": "*.rust-markdown-notebook.md"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "rust_markdown_notebook_wasm": "link:../rust_markdown_notebook_wasm/pkg"
  }
}
